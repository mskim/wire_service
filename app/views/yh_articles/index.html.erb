<p id="notice"><%= notice %></p>

<h1>연합기사</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Category</th>
      <th>Class code</th>
      <th>Source</th>
      <th>Title</th>
      <th>Writer</th>
      <th>Taken by</th>
      <th>Selected</th>
      <th>Group</th>
      <th>Page</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @yh_articles.each do |yh_article| %>
      <tr>
        <td><%= yh_article.date %></td>
        <td><%= yh_article.time %></td>
        <td><%= yh_article.category %></td>
        <td><%= yh_article.class_code %></td>
        <td><%= yh_article.source %></td>
        <td><%= yh_article.title %></td>
        <td><%= yh_article.writer %></td>
        <td><%= yh_article.taken_by %></td>
        <td><%= yh_article.selected %></td>
        <td><%= yh_article.group %></td>
        <td><%= yh_article.page %></td>
        <td><%= link_to 'Show', yh_article %></td>
        <td><%= link_to 'Edit', edit_yh_article_path(yh_article) %></td>
        <td><%= link_to 'Destroy', yh_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yh Article', new_yh_article_path %>

<div align="center">
  <%= paginate @yh_articles %>
</div>